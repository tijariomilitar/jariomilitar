			lib.display("story-3", "none");
			lib.display("story-4", "");
			lib.display("flow-next", "none");
			return Story.flow.status = "story-4";
		};
	};

	Story.flow.prev = () => {
		if(Story.flow.status == "story-2") {
			lib.display("story-2", "none");
			lib.display("story-1", "");
			lib.display("flow-prev", "none");
			return Story.flow.status = "story-1";
		};

		if(Story.flow.status == "story-3") {
			lib.display("story-3", "none");
			lib.display("story-2", "");
			return Story.flow.status = "story-2";
		};

		if(Story.flow.status == "story-4") {
			lib.display("story-4", "none");
			lib.display("story-3", "");
			lib.display("flow-next", "");
			return Story.flow.status = "story-3";
		};
	};
</script>
